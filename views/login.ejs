        <title>Login Page</title>

        <style>
            .intrologin{
                margin: 0 auto 0 auto;
                width:830px;
                height: calc(100vw * 0.436);
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
            }   
            .introtext{
                color: #FFF;
                text-align: center;
                font-family: Montserrat;    
                font-size: 96px;
                font-style: normal;
                font-weight: 700;
                line-height: normal;
                margin-bottom: 37.6px;
            }
            input{
                box-sizing: border-box;
            }
            #username{
                padding-left: 15px;
                border: 0;
                width: 377px;
                height: 31px;
                flex-shrink: 0;
                border-radius: 30px;
                background: #FFF;
                box-shadow: 0px 5px 4px 0px rgba(0, 0, 0, 0.25);
                color: rgba(0, 0, 0, 0.50);
                font-family: Montserrat;
                font-size: 14px;
                font-style: normal;
                font-weight: 400;
                line-height: normal;
            }
            #password{
                padding-left: 15px;
                border: 0;
                width: 377px;
                height: 31px;
                flex-shrink: 0;
                border-radius: 30px;
                background: #FFF;
                box-shadow: 0px 5px 4px 0px rgba(0, 0, 0, 0.25);
                color: rgba(0, 0, 0, 0.50);
                font-family: Montserrat;
                font-size: 14px;
                font-style: normal;
                font-weight: 400;
                line-height: normal;
            }
            .gaga{
                position: absolute;
                top: 33%;
                right: 2%;
            }
            .login-button{
                box-sizing: border-box;
                border-radius: 39.278px;
                background: #000;
                box-shadow: 0px 6.54639196395874px 5.2371134757995605px 0px rgba(0, 0, 0, 0.25);
                display: flex;
                width: 127px;
                height: 40.588px;
                padding: 15.711px 41.897px;
                justify-content: center;
                align-items: center;
                gap: 15.711px;
                flex-shrink: 0;
                color: #FFF;
                text-align: center;
                font-feature-settings: 'cv04' on, 'cv03' on, 'cv02' on, 'cv11' on, 'clig' off, 'liga' off;
                font-family: Montserrat;
                font-size: 20.948px;
                font-style: normal;
                font-weight: 400;
                line-height: 150%; /* 31.423px */
            }
            .createacc{
                display: flex;
                flex-direction: column;
                align-items: center;
                width: 100%;
                height: 300px;
                box-sizing: border-box;
                padding-top: 30px;
            }
        </style>

    </head>
